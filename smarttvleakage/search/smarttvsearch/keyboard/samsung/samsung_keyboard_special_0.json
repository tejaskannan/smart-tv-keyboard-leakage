{
    "adjacency_list": {
        "<NEXT>": {"down": "<CHANGE>", "right": "1"},
        "1": {"down": "!", "left": "<NEXT>", "right": "2"},
        "2": {"left": "1", "down": "@", "right": "3"},
        "3": {"left": "2", "down": "#", "right": "4"},
        "4": {"left": "3", "down": "$", "right": "5"},
        "5": {"left": "4", "down": "/", "right": "6"},
        "6": {"left": "5", "down": "^", "right": "7"},
        "7": {"left": "6", "down": "&", "right": "8"},
        "8": {"left": "7", "down": "*", "right": "9"},
        "9": {"left": "8", "down": "(", "right": "0"},
        "0": {"left": "9", "down": ")", "right": "<BACK>"},
        "<BACK>": {"down": "[", "left": "0"},
        "<CHANGE>": {"up": "<NEXT>", "right": "!"},
        "!": {"left": "<CHANGE>", "up": "1", "right": "@", "down": "'"},
        "@": {"left": "!", "up": "2", "down": "\"", "right": "#"},
        "#": {"left": "@", "up": "3", "down": ":", "right": "$"},
        "$": {"left": "#", "up": "4", "right": "/", "down": ";"},
        "/": {"left": "$", "up": "5", "down": ",", "right": "^"},
        "^": {"left": "/", "up": "6", "down": "?", "right": "&"},
        "&": {"left": "^", "up": "7", "right": "*", "down": "<"},
        "*": {"left": "&", "up": "8", "down": ">", "right": "("},
        "(": {"left": "*", "up": "9", "down": "{", "right": ")"},
        ")": {"left": "(", "up": "0", "down": "}", "right": "["},
        "[": {"up": "<BACK>", "left": ")", "down": "<<", "right": "]"},
        "]": {"left": "[", "down": ">>"},
        "'": {"up": "!", "right": "\"", "down": "-", "left": "<LANGUAGE>"},
        "\"": {"left": "'", "up": "@", "down": "+", "right": ":"},
        ":": {"left": "\"", "up": "#", "right": ";", "down": "<MULT>"},
        ";": {"left": ":", "up": "$", "down": "<DIV>", "right": ","},
        ",": {"left": ";", "up": "/", "right": "?", "down": "="},
        "?": {"left": ",", "up": "^", "right": "<", "down": "%"},
        "<": {"left": "?", "up": "&", "down": "\\", "right": ">"},
        ">": {"left": "<", "up": "*", "right": "{", "down": "<EURO>"},
        "{": {"left": ">", "up": "(", "right": "}", "down": "<POUND>"},
        "}": {"down": "<YEN>", "left": "{", "up": ")", "right": "<<"},
        "<<": {"up": "[", "left": "}", "right": ">>", "down": "<UP>"},
        ">>": {"left": "<<", "up": "]", "down": "<CENT>"},
        "-": {"up": "'", "right": "+", "left": "<LANGUAGE>", "down": "<SETTINGS>"},
        "+": {"up": "\"", "left": "-", "right": "<MULT>", "down": "<SETTINGS>"},
        "<MULT>": {"left": "+", "up": ":", "right": "<DIV>", "down": "<SPACE>"},
        "<DIV>": {"left": "<MULT>", "up": ";", "right": "=", "down": "<SPACE>"},
        "=": {"left": "<DIV>", "up": ",", "right": "%", "down": "<SPACE>"},
        "%": {"left": "=", "up": "?", "right": "\\", "down": "<SPACE>"},
        "\\": {"left": "%", "up": "<", "right": "<EURO>"},
        "<EURO>": {"left": "\\", "up": ">", "right": "<POUND>"},
        "<POUND>": {"up": "{", "left": "<EURO>", "right": "<YEN>"},
        "<YEN>": {"up": "}", "left": "<POUND>", "down": "<LEFT>", "right": "<UP>"},
        "<UP>": {"left": "<YEN>", "right": "<CENT>", "down": "<DOWN>", "up": "<<"},
        "<CENT>": {"left": "<UP>", "down": "<RIGHT>", "up": ">>"},
        "<LEFT>": {"right": "<DOWN>", "up": "<YEN>"},
        "<DOWN>": {"left": "<LEFT>", "up": "<UP>", "right": "<RIGHT>"},
        "<RIGHT>": {"left": "<DOWN>", "up": "<CENT>"},
        "<SPACE>": {"up": "<MULT>"},
        "<WWW>": {"up": "\\", "right": "<COM>"},
        "<COM>": {"up": "<EURO>", "right": "<SLASH>"},
        "<SLASH>": {"right": "<LEFT>", "up": "<POUND>", "left": "<COM>"},
        "<DONE>": {"left": ">>", "up": "<RETURN>", "down": "<CANCEL>"},
        "<CANCEL>": {"left": "<RIGHT>", "down": "<DONE>"},
        "<RETURN>": {"left": "]", "up": "<DELETEALL>", "down": "<DONE>"},
        "<DELETEALL>": {"left": "<BACK>", "down": "<RETURN>"},
        "<SETTINGS>": {"up": "-", "right": "<SPACE>"},
        "<LANGUAGE>": {"right": "'"}
    },
    "shortcuts": [{
        "<CHANGE>": { "down": "<LANGUAGE>" },
        "<LANGUAGE>": { "up": "<CHANGE>" },
        "<SPACE>": { "left": "<SETTINGS>", "right": "<WWW>" },
        "<CENT>": { "right": "<DONE>" },
        "<SETTINGS>": { "left": "<LANGUAGE>", "right": "<SPACE>" },
        "<WWW>": { "left": "<SPACE>" },
        "<DIV>": { "down": "<SPACE>" },
        "=": { "down": "<SPACE>" },
        "%": { "down": "<SPACE>" },
        "]": { "right": "<BACK>" }
    }],
    "wraparound": {
        "<NEXT>": { "left": "<DELETEALL>" },
        "<DELETEALL>":  {"right": "<NEXT>" },
