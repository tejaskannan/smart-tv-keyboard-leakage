        "<RETURN>": { "right": "<CHANGE>" },
        "<LANGUAGE>": { "left": "<DONE>" },
        "<DONE>": { "right": "<LANGUAGE>" }
    },
    "unclickable": ["<LANGUAGE>", "<SETTINGS>", "<UP>", "<DOWN>", "<RETURN>"]
